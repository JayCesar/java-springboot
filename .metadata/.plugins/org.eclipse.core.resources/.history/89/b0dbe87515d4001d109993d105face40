package exercises;

import java.util.Locale;
import java.util.Scanner;

public class le_item_e_codigo {
	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int codItem = sc.nextInt();
		int quantItem = sc.nextInt();
		double total;
		
		if (codItem == 3) {
			total = (double) quantItem * 4.00;
			System.out.printf("Total = R$ %.2f%n", total);
		}else if(codItem == 2) {
			total = (double) quantItem * 4.50;
			System.out.printf("Total = R$ %.2f%n", total);
		}else if (codItem == 3) {
			total = (double) quantItem * 5.00;
			System.out.printf("Total = R$ %.2f%n", total);
		}else if (codItem == 4) {
			total = (double) quantItem * 2.00;
			System.out.printf("Total = R$ %.2f%n", total);
		}else if (codItem == 5) {
			total = (double) quantItem * 1.50;
			System.out.printf("Total = R$ %.2f%n", total);
		}
		
		sc.close();
	}
}
