package classes;

import java.util.Scanner;

public class estrutura_while {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();

		int soma = 0;
		while (x != 0) {
			soma += x; // operador de atribuição cumulativo
			x = sc.nextInt();
		}

		System.out.println(soma);
		sc.close();

	}

}
